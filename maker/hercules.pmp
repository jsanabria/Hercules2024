        return CurrentTable()->{$fieldName}->CurrentValue ?? '';
    };
    
    // Función auxiliar para formatear la fecha de forma segura
    $format_date_safe = function ($value) {
        $timestamp = strtotime($value);
        return ($timestamp !== false) 
            ? date("d/m/Y h:i:sA", $timestamp) 
            : "—"; // Retorna un guion o el texto de error que prefieras
    };

    // 1. Obtener valores de forma segura
    $fecha_cierre_val = $get_safe_value("fecha_cierre");
    $fecha_resg_val   = $get_safe_value("fecha");

    // --- Lógica para fec_cier (fecha_cierre) ---
    // Si el valor es "0000-00-00 00:00:00" O está vacío (que es lo que obtenemos si era null)
    if ($fecha_cierre_val == "0000-00-00 00:00:00" || $fecha_cierre_val == "") {
        $this->ListOptions->Items["fec_cier"]->Body = "";
    } else {
        // El valor ya no es null y no es la fecha cero, podemos formatear con seguridad.
        $this->ListOptions->Items["fec_cier"]->Body = $format_date_safe($fecha_cierre_val);
    }

    // --- Lógica para fec_resg (fecha) ---
    // Formateamos directamente el valor seguro
    $this->ListOptions->Items["fec_resg"]->Body = $format_date_safe($fecha_resg_val);
}]]></CustomScript>
  <CustomScript ScriptType="Server" ScriptCodeType="Table" ScriptName="ListOptions_Rendered" ScriptCtrlID="list" ScriptL1Key="sco_seguimiento"><![CDATA[// ListOptions Rendered event
function ListOptions_Rendered() {
    
    // Función auxiliar para obtener el valor de forma segura, pasando "" si es null.
    // Esto evita el Deprecated de trim() y strtotime().
    $get_safe_value = function ($field) {
        // Usa el operador de fusión de nulos (?? '')
        return CurrentTable()->{$field}->CurrentValue ?? '';
    };

    // Obtenemos los valores de forma segura
    $fecha_cierre_val = $get_safe_value("fecha_cierre");
    $fecha_resg_val   = $get_safe_value("fecha");

    // --- Lógica para fecha_cierre ---
    // Chequeamos si el valor es el string "0000-00-00 00:00:00" O si es una cadena vacía (si era NULL).
    if (trim($fecha_cierre_val) == "0000-00-00 00:00:00" || trim($fecha_cierre_val) == "") {
        $this->ListOptions->Items["fec_cier"]->Body = "";
    } else {
        // Si tiene un valor, lo formateamos. Se verifica que strtotime no falle, aunque 
        // ya está protegido de NULL gracias a $get_safe_value.
        $timestamp = strtotime($fecha_cierre_val);
        $this->ListOptions->Items["fec_cier"]->Body = ($timestamp !== false) 
            ? date("d/m/Y h:i:sA", $timestamp) 
            : "Error de Fecha";
    }

    // --- Lógica para fecha ---
    // Verificamos que strtotime no falle (recuerda que $fecha_resg_val ya está protegido de NULL)
    $timestamp = strtotime($fecha_resg_val);
    $this->ListOptions->Items["fec_resg"]->Body = ($timestamp !== false) 
        ? date("d/m/Y h:i:sA", $timestamp) 
        : "Error de Fecha";
}]]></CustomScript>
