  <CustomScript ScriptType="Client" ScriptCodeType="Table" ScriptName="Startup Script" ScriptCtrlID="add" ScriptL1Key="sco_tasa_usd"><![CDATA[$(document).ready(function() {
    $("#x_tasa").on("keypress", function(e) {
        // Permitir: backspace, delete, tab, escape, enter y el punto (.)
        // El código del punto es 46
        if ($.inArray(e.keyCode, [46, 8, 9, 27, 13]) !== -1 ||
            // Permitir: Ctrl+A, Ctrl+C, Ctrl+V, Ctrl+X, Home, End
            (e.ctrlKey === true || e.metaKey === true)) {
            
            // Si es un punto, verificar que no exista ya uno en el campo
            if (e.which === 46 && $(this).val().indexOf('.') !== -1) {
                e.preventDefault();
            }
            return;
        }

        // Bloquear si no es un número (teclas entre 48 y 57)
        if ((e.shiftKey || (e.which < 48 || e.which > 57))) {
            e.preventDefault();
        }
    });

    // Filtro de limpieza para cuando pegan texto con el mouse
    $("#x_tasa").on("input", function() {
        let self = $(this);
        // Elimina cualquier cosa que no sea número o punto
        let validVal = self.val().replace(/[^0-9.]/g, '');
        
        // Evita que haya más de un punto si pegaron algo como "10.50.2"
        let parts = validVal.split('.');
        if (parts.length > 2) {
            validVal = parts[0] + '.' + parts.slice(1).join('');
        }
        
        if (self.val() !== validVal) {
            self.val(validVal);
        }
    });
});]]></CustomScript>
