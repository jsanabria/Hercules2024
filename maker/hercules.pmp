  <CustomScript ScriptType="Server" ScriptCodeType="Table" ScriptName="Row_Inserting" ScriptL1Key="view_insumos"><![CDATA[// Row Inserting event
function Row_Inserting($rsold, &$rsnew) {
    
    // Obtener el valor del tipo de servicio
    $tipoServicio = $rsnew["tipo"] ?? '';

    // Cancelar si el tipo es inválido
    if (empty($tipoServicio)) {
        $this->CancelMessage = "El campo 'tipo' es requerido para generar el código de servicio.";
        return FALSE;
    }
    
    // 1. CORRECCIÓN CRÍTICA: Sanitizar el valor antes de inyectar en la consulta SQL
    $tipoServicio_safe = adjustSql($tipoServicio); 

    // Consulta SQL para obtener el consecutivo máximo para este tipo
    // Se usa LENGTH(tipo) + 1 para hacerlo más robusto si el prefijo 'tipo' no siempre tiene 4 caracteres.
    $sql = "
        SELECT 
            MAX(
                CONVERT(
                    SUBSTRING_INDEX(
                        SUBSTRING(Nservicio, LENGTH(tipo) + 1),
                        '-',
                        -1
                    ),
                    UNSIGNED INTEGER
                )
            ) + 1 AS CODIGO
        FROM sco_servicio 
        WHERE tipo = '" . $tipoServicio_safe . "';
    ";
    
    $CODIGO = ExecuteScalar($sql);
    
    // Si la consulta devuelve NULL (primer registro del tipo), usa 1.
    $nuevoConsecutivo = intval($CODIGO ?? 1); 
    
    // Formatear el consecutivo (6 dígitos con relleno de ceros) y actualizar el campo
    $rsnew["Nservicio"] = $tipoServicio . str_pad($nuevoConsecutivo, 6, '0', STR_PAD_LEFT);
    
    return TRUE;
}]]></CustomScript>
