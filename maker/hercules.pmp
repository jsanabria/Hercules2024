  <CustomScript ScriptType="Server" ScriptCodeType="Table" ScriptName="ListOptions_Rendered" ScriptCtrlID="list" ScriptL1Key="view_insumos"><![CDATA[// ListOptions Rendered event
function ListOptions_Rendered() {
    
    // Obtener y sanitizar todas las variables de entrada
    $fecha_desde = $_REQUEST["fecha_desde"] ?? '';
    $fecha_hasta = $_REQUEST["fecha_hasta"] ?? '';
    $servicioId = $this->Nservicio->CurrentValue ?? '';

    // Sanitizar las variables para SQL
    $servicioId_safe = adjustSql($servicioId);
    $fecha_desde_safe = adjustSql($fecha_desde);
    $fecha_hasta_safe = adjustSql($fecha_hasta);

    // Inicializar variables de filtro
    $bt_rango = ""; // Filtro para Entradas y Salidas (Rango)
    $bt_corte = ""; // Filtro para Disponible (Corte en fecha_hasta)

    // Lógica de armado de filtros. Solo si ambas fechas están presentes y tienen el formato esperado.
    if (strlen($fecha_desde) === 10 && strlen($fecha_hasta) === 10) {
        // Se asume que las fechas están en formato 'YYYY-MM-DD' para BETWEEN
        $bt_rango = "AND b.fecha BETWEEN '$fecha_desde_safe' AND '$fecha_hasta_safe'";
        $bt_corte = "AND b.fecha <= '$fecha_hasta_safe'";
    }
    
    // Si el ID del servicio está vacío, no se puede consultar.
    if (empty($servicioId)) {
        return;
    }

    // --- CONSOLIDACIÓN Y OPTIMIZACIÓN DE INVENTARIO ---
    
    // 1. Calcular Entradas y Salidas en el RANGO (bt_rango)
    $sql_rango = "
        SELECT 
            SUM(CASE WHEN a.cantidad >= 0 THEN a.cantidad ELSE 0 END) AS Entradas,
            SUM(CASE WHEN a.cantidad < 0 THEN a.cantidad ELSE 0 END) AS Salidas
        FROM 
            sco_entrada_salida_detalle a 
            JOIN sco_entrada_salida b ON b.Nentrada_salida = a.entrada_salida 
        WHERE 
            a.articulo = '" . $servicioId_safe . "' $bt_rango;
    ";
    $rango_row = ExecuteRow($sql_rango);
    $entradas = (float)($rango_row["Entradas"] ?? 0);
    $salidas = (float)($rango_row["Salidas"] ?? 0);

    // 2. Calcular Disponible (Stock acumulado hasta la fecha de corte bt_corte)
    $sql_disponible = "
        SELECT 
            SUM(a.cantidad) AS Disponible
        FROM 
            sco_entrada_salida_detalle a 
            JOIN sco_entrada_salida b ON b.Nentrada_salida = a.entrada_salida 
        WHERE 
            a.articulo = '" . $servicioId_safe . "' $bt_corte;
    ";
    $disponible = ExecuteScalar($sql_disponible);
    $disponible = (float)($disponible ?? 0);


    // --- ASIGNACIÓN DE RESULTADOS ---
    
    // 1. Disponible (Stock acumulado hasta la fecha de corte)
    $this->ListOptions->Items["Disponible"]->Body = number_format($disponible, 0, "", ".");

    // 2. Entradas (En el rango: con link)
    // Se sanitizan las variables para la URL (aunque ya se sanitizaron, htmlspecialchars es bueno para URLs)
    $Nservicio_url = htmlspecialchars($servicioId);
    $fecha_desde_url = htmlspecialchars($fecha_desde);
    $fecha_hasta_url = htmlspecialchars($fecha_hasta);
    
    $link_entradas = 'fpdf_report/rptEntradaSalidaInsumos.php?Nservicio=' . $Nservicio_url . 
                     '&fecha_desde=' . $fecha_desde_url . 
                     '&fecha_hasta=' . $fecha_hasta_url;
                     
    $this->ListOptions->Items["Entradas"]->Body = '<a href="' . $link_entradas . '" target="_blank">' . number_format($entradas, 0, "", ".") . '</a>';

    // 3. Salidas (En el rango: se muestra el valor absoluto)
    // Nota: El SQL calcula el SUM(cantidad < 0), por lo que se usa abs() si queremos mostrar el valor positivo de salidas.
    $this->ListOptions->Items["Salidas"]->Body = number_format(abs($salidas), 0, "", ".");
}]]></CustomScript>
  <CustomScript ScriptType="Server" ScriptCodeType="Table" ScriptName="ListOptions_Rendered" ScriptCtrlID="list" ScriptL1Key="view_capillas"><![CDATA[// ListOptions Rendered event
function ListOptions_Rendered() {
    
    $servicioId = $this->Nservicio->CurrentValue ?? '';
    $tipoServicio = $this->tipo->CurrentValue ?? '';
    
    // 1. Sanitize input for security
    if (empty($servicioId)) {
        return;
    }
    $servicioId_safe = adjustSql($servicioId);

    // 2. Check for "CAP" type services
    if (substr($tipoServicio, 0, 3) === "CAP") {
        
        // CORRECTION: Use a single query to fetch all tariffs, avoiding the N+1 loop.
        $sql_tarifas = "
            SELECT 
                horas, 
                precio 
            FROM 
                sco_tarifa
            WHERE 
                servicio = '" . $servicioId_safe . "' AND activo = 'S'
            ORDER BY horas ASC;
        ";
        
        $tarifas_rs = ExecuteRows($sql_tarifas);
        $precioBody = "";

        if (!empty($tarifas_rs)) {
            // Iterate efficiently over the fetched rows
            foreach ($tarifas_rs as $row) {
                // Build the output string
                $precioBody .= $row["horas"] . " Horas: Bs. " . 
                                number_format($row["precio"], 2, ",", ".") . " / ";
            }
            
            // Assign the formatted string, removing the trailing " / "
            $this->ListOptions->Items["Precio"]->Body = rtrim($precioBody, " / ");
        } else {
            // Handle case where type is CAP but no tariffs are found
            $this->ListOptions->Items["Precio"]->Body = 'No hay tarifas asociadas';
        }
    } 
    // If the block is part of a larger script, you'd add the 'else' logic here
    // else { ... other pricing logic ... }
}]]></CustomScript>
