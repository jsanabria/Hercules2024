  <CustomScript ScriptType="Server" ScriptCodeType="Table" ScriptName="ListOptions_Rendered" ScriptCtrlID="preview" ScriptL1Key="sco_orden"><![CDATA[// ListOptions Rendered event
function ListOptions_Rendered() {
    // Definimos una función helper para manejar los valores nulos o vacíos.
    // Esto asegura que strtotime() reciba una cadena (vacía si es null/vacío),
    // previniendo el error de Deprecated.
    $safe_strtotime = function ($value) {
        // Usa el operador de fusión de nulos (??) para asegurar una cadena vacía si es null
        $safe_value = $value ?? '';
        
        // Verifica que la cadena no esté vacía antes de llamar a strtotime
        return !empty($safe_value) ? strtotime($safe_value) : false;
    };
    
    // Obtener valores de fecha/hora de forma segura
    $fecha_inicio_ts = $safe_strtotime($this->fecha_inicio->CurrentValue);
    $hora_inicio_ts  = $safe_strtotime($this->hora_inicio->CurrentValue);
    $fecha_fin_ts    = $safe_strtotime($this->fecha_fin->CurrentValue);
    $hora_fin_ts     = $safe_strtotime($this->hora_fin->CurrentValue);

    // Formateo seguro para la fecha
    $format_date = function ($timestamp) {
        return $timestamp !== false ? date('d/m/Y', $timestamp) : '—';
    };
    // Formateo seguro para la hora
    $format_time = function ($timestamp) {
        return $timestamp !== false ? date('h:iA', $timestamp) : '—';
    };

    switch($this->paso->CurrentValue) {
    case 1:
        $inicio_str = 'Inicia: ' . $format_date($fecha_inicio_ts) . ' ' . $format_time($hora_inicio_ts);
        $fin_str = 'Finaliza: ' . $format_date($fecha_fin_ts) . ' ' . $format_time($hora_fin_ts);
        $this->ListOptions->Items["new2"]->Body = $inicio_str . '<br />' . $fin_str;
        break;
    case 2:
    case 5:
    case 6:
        // Combinamos la fecha de fin y hora de fin de forma segura
        $this->ListOptions->Items["new2"]->Body = $format_date($fecha_fin_ts) . ' ' . $format_time($hora_fin_ts);
        break;
    case 3:
    case 4:
        $this->ListOptions->Items["new2"]->Body = "N/A";
        break;
    }

    $value = ExecuteScalar("SELECT nombre FROM sco_proveedor WHERE Nproveedor = '" . $this->proveedor->CurrentValue . "';");
    $this->ListOptions->Items["new"]->Body = $value;
}]]></CustomScript>
