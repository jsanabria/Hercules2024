  <CustomScript ScriptType="Server" ScriptCodeType="Table" ScriptName="Row_Updating" ScriptL1Key="view_parcela_ventas"><![CDATA[function Row_Updating($rsold, &$rsnew) {
    $userLevel = CurrentUserLevel();
    $userName = CurrentUserName();
    $hoy = CurrentDate();

    // --- CASO 1: PROCESO DE ANULACIÓN ---
    if ($rsnew["estatus"] == "ANULADO" && $rsold["estatus"] != "ANULADO") {
        $canAnular = ExecuteScalar("SELECT b.codigo FROM sco_grupo_funciones AS a 
                                   JOIN sco_funciones AS b ON b.Nfuncion = a.funcion 
                                   WHERE a.grupo = '$userLevel' AND b.codigo = '200'");
        
        if ($canAnular == "200") {
            $rsnew["nota"] = ($rsold["nota"] ?? "") . "\n[LOG]: Usuario $userName ANULÓ la venta el " . CurrentDateTime() . ".";
        } else {
            $this->setFailureMessage("No tiene nivel de seguridad (Cód: 200) para ANULAR.");
            return FALSE;
        }
    }

    // --- CASO 2: REVERSO A COMPRA (REVERTIR) ---
    if ($rsnew["estatus"] == "COMPRA" && $rsold["estatus"] == "VENTA") {
        $canRevertir = ExecuteScalar("SELECT b.codigo FROM sco_grupo_funciones AS a 
                                     JOIN sco_funciones AS b ON b.Nfuncion = a.funcion 
                                     WHERE a.grupo = '$userLevel' AND b.codigo = '100'");
        
        if ($canRevertir == "100") {
            $rsnew["usuario_vende"] = "";
            $rsnew["fecha_venta"] = "1990-01-01";
            $rsnew["moneda_venta"] = "";
            $rsnew["valor_venta"] = 0.00;
            $rsnew["tasa_venta"] = 0.00;
            $rsnew["ci_comprador"] = "";
            $rsnew["comprador"] = "";
            $rsnew["nota"] = ($rsold["nota"] ?? "") . "\n[LOG]: Usuario $userName REVIRTIÓ a COMPRA el " . CurrentDateTime() . ".";
        } else {
            $this->setFailureMessage("No está autorizado (Cód: 100) para REVERTIR esta parcela.");
            return FALSE;
        }
    }

    // Si es una venta normal (de estatus vacío o compra a venta)
    if ($rsold["estatus"] == "COMPRA" && $rsnew["estatus"] == "VENTA") {
        $rsnew["fecha_venta"] = $hoy;
        $rsnew["usuario_vende"] = $userName;
        $rsnew["estatus"] = "VENTA";
    }

    // Bitácora
    $nota = "Campos modificados: \n";
    $cambioDetectado = false;

    // Recorremos solo los campos que vienen en $rsnew (los que cambiaron)
    foreach ($rsnew as $campo => $valorNuevo) {
        $valorAnterior = $rsold[$campo];

        // Solo registramos si los valores son realmente distintos
        if ($valorAnterior != $valorNuevo) {
            // Formateamos valores nulos para que sean legibles en la nota
            $txtViejo = is_null($valorAnterior) ? "NULL" : $valorAnterior;
            $txtNuevo = is_null($valorNuevo) ? "NULL" : $valorNuevo;

            $nota .= "- $campo: de '$txtViejo' a '$txtNuevo'\n";
            $cambioDetectado = true;
        }
    }

    // Si no hubo cambios reales, no insertamos nada
    if (!$cambioDetectado) return;

    // Escapamos la nota para evitar errores con comillas simples en el SQL
    $notaEscapada = str_replace("'", "''", $nota);
    $idVenta = $rsold["Nparcela_ventas"];
    $usuario = CurrentUserName();
    $fecha = CurrentDateTime();

    $sql = "INSERT INTO sco_parcela_ventas_nota
                (Nparcela_ventas_nota, parcela_ventas, nota, username, fecha_hora)
            VALUES 
                (NULL, $idVenta, '$notaEscapada', '$usuario', '$fecha')";
    
    Execute($sql);

    return TRUE;
}]]></CustomScript>
