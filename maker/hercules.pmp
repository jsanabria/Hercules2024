  <CustomScript ScriptType="Server" ScriptCodeType="Table" ScriptName="ListOptions_Rendered" ScriptCtrlID="list" ScriptL1Key="view_reclamo_lapida"><![CDATA[function ListOptions_Rendered() {
    // 1. Cálculo de tiempo transcurrido
    $fd = $this->registro->CurrentValue ?? date("Y-m-d");
    $fh = date("Y-m-d");
    
    // Aseguramos que el ítem "new" exista antes de asignar
    if (isset($this->ListOptions->Items["new"])) {
        $this->ListOptions->Items["new"]->Body = tiempoTranscurridoFechas($fd, $fh);
    }

    // 2. Control del botón Editar (Ocultar si es necesario)
    if (isset($this->ListOptions->Items["edit"])) {
        $this->ListOptions->Items["edit"]->Visible = false; // Es más limpio que asignar Body vacío
    }

    // 3. Botón: Ver Reclamo (Visualización)
    if (isset($this->ListOptions->Items["ver_reclamo"])) {
        // Usamos ?? "" para convertir null en string vacío y evitar el error Deprecated
        $idReclamo = urlencode($this->Nreclamo->CurrentValue ?? ""); 
        
        $urlVer = "ScoReclamoView/" . $idReclamo . "?showdetail="; 
        $this->ListOptions->Items["ver_reclamo"]->Body = sprintf(
            '<a class="btn btn-default ewRowLink ewView" title="Ver Reclamo" target="_blank" href="%s">
                <i class="fas fa-search"></i>
            </a>',
            $urlVer
        );
    }

    // 4. Botón: Crear Registro
    if (isset($this->ListOptions->Items["crear_registro"])) {
        $idReclamo = urlencode($this->Nreclamo->CurrentValue ?? "");
        
        $urlCrear = "ViewReclamoLapidaEdit?Nreclamo=" . $idReclamo;
        $this->ListOptions->Items["crear_registro"]->Body = sprintf(
            '<a class="btn btn-default ewRowLink ewEdit" title="Crear Registro de Lapidas" href="%s">
