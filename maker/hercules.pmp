  <CustomScript ScriptType="Server" ScriptCodeType="Table" ScriptName="Page_Load" ScriptCtrlID="list" ScriptL1Key="view_sms_parcelas"><![CDATA[// Page Load event
function Page_Load() {
	//echo "Page Load";
	if(isset($_POST["prepa"])) {
		include "dashboard/clases/sms.class.php";
		$sw=false;
		foreach ($_POST["prepa"] as $key => $value) {
			$inf = explode("|", $value);
			$celular = $inf[0];
			$comando = $inf[1];
			$cedula = $inf[2];
			$titular = $inf[3];
			$texto = $inf[4];
			//echo "$celular -- $texto <br>";
			if(trim($texto) != "") {
				$cel1 = $celular; 
				$cel1 = str_replace("(", "", $cel1);
				$cel1 = str_replace(")", "", $cel1);
				$cel1 = str_replace("-", "", $cel1);
				$cel1 = str_replace(" ", "", $cel1); 
				if(trim($cel1)!="") { 
					if(substr($cel1,0,4)=="0412" or substr($cel1,0,4)=="0414" or substr($cel1,0,4)=="0424" or substr($cel1,0,4)=="0416" or substr($cel1,0,4)=="0426") {
						$sms = new sms(); 
						$sms->enviar_sms($cel1, $texto);
						unset($sms);
						$sql = "INSERT INTO sco_sms
									(Nsms, celular, comando, cedula, titular, texto, fecha)
								VALUES (NULL, '$cel1', '$comando', '$cedula', '$titular', '$texto', NOW())";
								Execute($sql);
						$sw=true;
					}
				}
			}
		}
		if($sw) {
			echo '<script> alert("Se Enviaron los SMS correctamente!"); </script>';
		}
	}
}
]]></CustomScript>
