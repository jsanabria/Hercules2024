  <CustomScript ScriptType="Server" ScriptCodeType="Table" ScriptName="Row_Inserted" ScriptL1Key="sco_expediente_estatus"><![CDATA[// Row Inserted event
function Row_Inserted($rsold, $rsnew) {
	//echo "Row Inserted"
	$sql = "UPDATE sco_expediente
			SET estatus = '".$rsnew["estatus"]."'
			WHERE Nexpediente = '".$rsnew["expediente"]."'";
	Execute($sql);

	// Si se anula el expedeinte, se verifican si se asignó ataud y/o cofre
	// para realizar el reverso de los mismo y eliminar esos registros
	// paso 3 del expediente

	if($rsnew["estatus"]==7) {
		$sql = "SELECT count(servicio) as cantidad FROM sco_orden WHERE paso = 3 AND expediente = '".$rsnew["expediente"]."'";
		$cantidad = ExecuteScalar($sql);
		for($i=0; $i<$cantidad; $i++) {
			$sql = "SELECT servicio FROM sco_orden WHERE paso = 3 AND expediente = '".$rsnew["expediente"]."' LIMIT $i,1";
			$serv = ExecuteScalar($sql);

			$sql = "INSERT INTO sco_entrada_salida
						(Nentrada_salida, tipo_doc, proveedor, documento, fecha, nota, username)
					VALUES (NULL, 'EXIN', -1, '".$rsnew["expediente"]."', NOW(), 'ENTRADA AUTOMATICA POR ANULACION DE Exp.: ".$rsnew["expediente"]."', '".CurrentUserName()."')";
			Execute($sql);
			$idEntr = ExecuteScalar("SELECT LAST_INSERT_ID();");
	
			$sql = "INSERT INTO sco_entrada_salida_detalle
						(Nentrada_salida_detalle, entrada_salida, tipo_doc, proveedor, articulo, cantidad, costo, total)
					VALUES (NULL, $idEntr, 'EXIN', -1, '$serv', 1, 0, 0)";
			Execute($sql);
		}
		$sql = "DELETE FROM sco_orden WHERE expediente = '".$rsnew["expediente"]."'";
		Execute($sql);

		// Descarga Sleeping Bag
		$sql = "INSERT INTO sco_entrada_salida
					(Nentrada_salida, tipo_doc, proveedor, documento, fecha, nota, username)
				VALUES (NULL, 'EXIN', -1, '" . $rsnew["expediente"] . "', NOW(), 'ENTRADA AUTOMATOMATICA POR ANULACION DE Exp.: " . $rs["expediente"] . "', '" . CurrentUserName() . "')";
		Execute($sql);
		$sql = "SELECT LAST_INSERT_ID()";
		$idSali = ExecuteScalar($sql);
		
		$sql = "INSERT INTO sco_entrada_salida_detalle
					(Nentrada_salida_detalle, entrada_salida, tipo_doc, proveedor, articulo, cantidad, costo, total)
				VALUES (NULL, $idSali, 'EXIN', -1, 'FUNE000001', 1, 0, 0)";
		Execute($sql);

		// Anulo las facturas asociadas al expediente
		$sql = "UPDATE sco_expediente_cia SET estatus = 1 WHERE expediente = '".$rsnew["expediente"]."';";
		Execute($sql);
	}


	// FIN
	
	// header("Location: sco_expediente_estatuslist.php?showmaster=sco_expediente&fk_Nexpediente=".$rsnew["expediente"]."");
	// die();
}
]]></CustomScript>
