  <CustomScript ScriptType="Server" ScriptCodeType="Table" ScriptName="ListOptions_Rendered" ScriptCtrlID="list" ScriptL1Key="sco_servicio"><![CDATA[// ListOptions Rendered event
function ListOptions_Rendered() {
    
    $servicioId = $this->Nservicio->CurrentValue ?? '';
    $tipoServicio = $this->tipo->CurrentValue ?? '';
    
    // Si el ID del servicio está vacío, no se puede consultar, salimos.
    if (empty($servicioId)) {
        return;
    }

    // --- 1. Lógica de Precio ---
    $this->ListOptions->Items["Precio"]->Body = '—'; // Valor por defecto
    
    // Bloque para servicios tipo "CAP" (Probablemente Capacitación/Tarifas por Hora)
    if (substr($tipoServicio, 0, 3) === "CAP") {
        
        // CORRECCIÓN: Usar una sola consulta para obtener todas las tarifas y evitar el bucle ineficiente.
        $sql_tarifas = "
            SELECT 
                horas, 
                precio 
            FROM 
                sco_tarifa
            WHERE 
                servicio = '" . adjustSql($servicioId) . "' AND activo = 'S'
            ORDER BY horas ASC;
        ";
        
        $tarifas_rs = ExecuteRows($sql_tarifas);
        $precioBody = "";

        if (!empty($tarifas_rs)) {
            foreach ($tarifas_rs as $row) {
                // Formato: "X Horas: Bs. YYY,YY / "
                $precioBody .= $row["horas"] . " Horas: Bs. " . 
                                number_format($row["precio"], 2, ",", ".") . " / ";
            }
            // Eliminar el último separador " / "
            $this->ListOptions->Items["Precio"]->Body = rtrim($precioBody, " / ");
        }

    } 
    // Bloque para otros servicios (asume una única tarifa)
    else {
        $sql_precio = "
            SELECT 
                precio 
            FROM 
                sco_tarifa 
            WHERE 
                servicio = '" . adjustSql($servicioId) . "' AND activo = 'S';
        ";
        $precio = ExecuteScalar($sql_precio);
        
        if ($precio !== false && $precio !== null) {
            $this->ListOptions->Items["Precio"]->Body = "Bs. " . number_format($precio, 2, ",", ".");
        }
    }

    // --- 2. Lógica de Inventario/Stock ---
    
    // Obtener todas las cantidades de una sola vez para evitar redundancia
    $sql_inventario = "
        SELECT 
            SUM(CASE WHEN cantidad >= 0 THEN cantidad ELSE 0 END) AS Entradas,
            SUM(CASE WHEN cantidad < 0 THEN cantidad ELSE 0 END) AS Salidas,
            SUM(cantidad) AS Disponible
        FROM 
            sco_entrada_salida_detalle 
        WHERE 
            articulo = '" . adjustSql($servicioId) . "';
    ";
    
    $inventario_row = ExecuteRow($sql_inventario);

    // Los valores de SUM() serán NULL si no hay registros, lo cual es manejado por number_format si convertimos a 0.
    $entradas = (float)($inventario_row["Entradas"] ?? 0);
    $salidas = (float)($inventario_row["Salidas"] ?? 0);
    $disponible = (float)($inventario_row["Disponible"] ?? 0);
    
    // Aplicar los resultados
    $this->ListOptions->Items["Disponible"]->Body = number_format($disponible, 0, "", ".");
    
    // Nota: Las salidas son números negativos en la base de datos (cantidad < 0), 
    // pero para la presentación deben ser mostrados como un valor positivo (e.g., 50, no -50).
    $this->ListOptions->Items["Entradas"]->Body = number_format($entradas, 0, "", ".");
    $this->ListOptions->Items["Salidas"]->Body = number_format(abs($salidas), 0, "", ".");
}]]></CustomScript>
