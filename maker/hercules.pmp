  <CustomScript ScriptType="Server" ScriptCodeType="Table" ScriptName="ListOptions_Rendered" ScriptCtrlID="list" ScriptL1Key="sco_reclamo"><![CDATA[// ListOptions Rendered event
function ListOptions_Rendered()
{
    // 1. Verificamos que la fecha de registro sea válida
    if (!EmptyValue($this->registro->CurrentValue)) {
        
        $fechaDesde = $this->registro->CurrentValue;
        $estatus = $this->estatus->CurrentValue;
        
        // Determinamos si el proceso terminó para fijar la fecha de corte
        $finalizado = in_array($estatus, ["RESUELTO", "RECHAZADO"]);
        
        // Si está finalizado usamos 'modificacion', de lo contrario la fecha actual
        $fechaHasta = ($finalizado && !EmptyValue($this->modificacion->CurrentValue)) 
            ? $this->modificacion->CurrentValue 
            : CurrentDate();

        // Acceso directo al ítem "new"
        if (isset($this->ListOptions->Items["new"])) {
            $this->ListOptions->Items["new"]->Body = tiempoTranscurridoFechas($fechaDesde, $fechaHasta);
        }
    }

    // 2. Validación de mensaje al cliente
    if (isset($this->ListOptions->Items["new2"])) {
        $tieneMensaje = trim($this->mensaje_cliente->CurrentValue ?? 0) !== 0;
        $this->ListOptions->Items["new2"]->Body = $tieneMensaje ? "SI" : "NO";
    }

    // 3. Conteo de adjuntos con seguridad SQL y Font Awesome 6
    if (isset($this->ListOptions->Items["new3"])) {
        $nReclamo = $this->Nreclamo->CurrentValue;
        
        // ExecuteScalar con AdjustSql para mayor seguridad
        $sql = "SELECT COUNT(*) FROM sco_reclamo_adjunto WHERE reclamo = '" . AdjustSql($nReclamo) . "'";
        $cnt = ExecuteScalar($sql);
        
        // Cambiamos a etiqueta <i> y clase de Font Awesome 6
        $this->ListOptions->Items["new3"]->Body = ($cnt > 0) 
            ? '<i class="fa-solid fa-paperclip text-primary" title="Tiene adjuntos"></i>' 
            : '';
    }
}]]></CustomScript>
