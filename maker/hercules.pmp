  <CustomScript ScriptType="Server" ScriptCodeType="Table" ScriptName="ListOptions_Rendered" ScriptCtrlID="list" ScriptL1Key="sco_orden_compra"><![CDATA[// ListOptions Rendered event
function ListOptions_Rendered() {
    
    $ordenCompraId = $this->Norden_compra->CurrentValue ?? '';

    // --- 1. Lógica de Impresión (Corrección del Icono) ---
    
    // CORRECCIÓN: Se reemplaza glyphicon-print por fa fa-print (Font Awesome)
    $linkBody = '<a target="_blank" href="fpdf_report/rptOrdenCompra.php?Norden_compra=' . htmlspecialchars($ordenCompraId) . '"><span class="fa fa-print"></span></a>';
    
    // Asignar el link solo si el estatus NO es ANULADO
    $this->ListOptions->Items["print"]->Body = ($this->estatus->CurrentValue == "ANULADO") ? "" : $linkBody;
    
    // --- 2. Lógica de Cálculo de Tiempo Transcurrido (Mantenida) ---
    
    $fd = $this->fecha->CurrentValue; 
    $fh = $this->fecha_aprobacion->CurrentValue; 
    
    if (empty($fh)) {
        $fh = date("Y-m-d"); 
    }
    
    if (!empty($fd) && !empty($fh)) {
        $this->ListOptions->Items["tiempo"]->Body = tiempoTranscurridoFechas($fd, $fh);
    } else {
        $this->ListOptions->Items["tiempo"]->Body = "N/A";
    }

    // --- 3. Lógica de Notas/Bitácora (Mantenida) ---
    
    $ordenCompraId_safe = adjustSql($ordenCompraId);
    
    $sql_notas = "
        SELECT COUNT(*) AS cnt 
        FROM sco_nota_orden_compra 
        WHERE orden_compra = '" . $ordenCompraId_safe . "' 
          AND NOT (nota LIKE 'SE HIZO CAMBIO DE ESTATUS%');
    ";
    
    $cnt = intval(ExecuteScalar($sql_notas));

    // Nota: Si los iconos de nota tampoco funcionan, también debes cambiar 
    // las clases glyphicon-tag y glyphicon-tags por fa fa-tag y fa fa-tags.
    $notaBody = '';
    if ($cnt == 1) {
        $notaBody = '<span class="fa fa-tag"></span>';
    } elseif ($cnt > 1) {
        $notaBody = '<span class="fa fa-tags"></span>';
    }
    $this->ListOptions->Items["nota"]->Body = $notaBody;
