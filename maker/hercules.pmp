    // --- 1. CONSULTA DE VEHÍCULO (Marca y Modelo) ---
    $n_flota = $this->flota->CurrentValue;
    $marca_modelo = "";
    
    if (!empty($n_flota)) {
        // Sanitización para prevenir Inyección SQL (asumiendo Nflota es INT)
        $n_flota_seguro = (int)$n_flota; 

        $sql_flota = "SELECT 
            b.nombre AS marca, c.nombre AS modelo 
        FROM 
            sco_flota AS a 
            LEFT OUTER JOIN sco_marca AS b ON b.Nmarca = a.marca 
            LEFT OUTER JOIN sco_modelo AS c ON c.marca = a.marca AND c.Nmodelo = a.modelo 
        WHERE a.Nflota = " . $n_flota_seguro;

        $row_flota = ExecuteRow($sql_flota);

        // Si se encuentra información, la preparamos para el encabezado
        if ($row_flota && (!empty($row_flota["marca"]) || !empty($row_flota["modelo"]))) {
            $marca_modelo = sprintf(
                '<div style="margin-bottom: 10px; font-weight: bold;">Vehículo: %s - %s</div>',
                htmlentities($row_flota["marca"]), // Sanitizar salida HTML
                htmlentities($row_flota["modelo"])
            );
        }
    }
    
    // --- 2. LÓGICA DE PROVEEDOR Y BOTÓN ---

    // 2.1. Sanitización de la clave de búsqueda (Proveedor)
    $n_proveedor = $this->proveedor->CurrentValue;

    if (empty($n_proveedor)) {
        // Si el Nproveedor está vacío, se muestra la info del vehículo (si existe) y el botón deshabilitado.
        $header = $marca_modelo . '<button id="send" name="send" type="button" class="btn btn-info btn-lg" disabled="disabled">Enviar Email (Proveedor no definido)</button>';
        return;
    }
    
    // Asumiendo que Nproveedor es un campo numérico (integer)
    $n_proveedor_seguro = (int)$n_proveedor; 

    // 2.2. Consulta Segura a la Base de Datos (Proveedor)
    $sql_proveedor = "SELECT nombre, correo, correo_adicional 
            FROM sco_proveedor 
            WHERE Nproveedor = " . $n_proveedor_seguro;
    
    $row_proveedor = ExecuteRow($sql_proveedor);

    // 2.3. Extracción de datos y manejo de errores (Proveedor)
    $proveedor = "";
    $email = "";
    $disabled = ' disabled="disabled"';
    $incidencia_id = (int)$this->Nflota_incidencia->CurrentValue; 

    if ($row_proveedor) {
        $proveedor = trim($row_proveedor["nombre"]);
        $email = trim($row_proveedor["correo"]);

        // Determinar si el botón debe estar habilitado
        if (!empty($proveedor) && !empty($email)) {
            $disabled = '';
        }
    }

    // 2.4. Construcción final del Header: Vehículo + Botón
    $header = $marca_modelo . sprintf(
        '<button id="send" name="send" type="button" class="btn btn-info btn-lg"%s 
            onClick="js:enviar_mail(%d, \'%s\');">
            Enviar Email al Proveedor %s
        </button>', 
        $disabled,
        $incidencia_id,
        $email,
        $proveedor
    );
}]]></CustomScript>
