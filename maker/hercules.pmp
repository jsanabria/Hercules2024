  <CustomScript ScriptType="Server" ScriptCodeType="Table" ScriptName="Row_Inserting" ScriptL1Key="view_capillas"><![CDATA[// Row Inserting event
function Row_Inserting($rsold, &$rsnew) {
    
    // Obtener el valor del tipo de servicio de forma segura
    $tipoServicio = $rsnew["tipo"] ?? '';

    // Si el tipo es esencialmente vacío, cancelamos (prevención de errores)
    if (empty($tipoServicio)) {
        // Asumiendo que esta tabla tiene una columna 'tipo'
        $this->CancelMessage = "El campo 'tipo' no puede estar vacío para generar el código de servicio.";
        return FALSE;
    }
    
    // 1. CORRECCIÓN CRÍTICA: Sanitizar el valor antes de inyectar en la consulta SQL
    $tipoServicio_safe = adjustSql($tipoServicio); 

    $sql = "
        SELECT 
            MAX(
                CONVERT(
                    SUBSTRING_INDEX(
                        SUBSTRING(Nservicio, LENGTH(tipo) + 1),
                        '-',
                        -1
                    ),
                    UNSIGNED INTEGER
                )
            ) + 1 AS CODIGO
        FROM sco_servicio 
        WHERE tipo = '" . $tipoServicio_safe . "';
    ";
    // Nota: Se ha usado LENGTH(tipo) + 1 en la consulta para ser más robusto si la longitud de 'tipo' varía.
    
    $CODIGO = ExecuteScalar($sql);
    
    // Si la consulta devuelve NULL (primer registro del tipo), usamos 1.
    $nuevoConsecutivo = intval($CODIGO ?? 1); 
    
    // Formatear el consecutivo (6 dígitos con relleno) y actualizar el campo
    $rsnew["Nservicio"] = $tipoServicio . str_pad($nuevoConsecutivo, 6, '0', STR_PAD_LEFT);
    $rsnew["nombre"] = strtoupper($rsnew["nombre"]);
    return TRUE;
}]]></CustomScript>
  <CustomScript ScriptType="Server" ScriptCodeType="Table" ScriptName="Row_Inserting" ScriptL1Key="view_velacion"><![CDATA[// Row Inserting event
function Row_Inserting($rsold, &$rsnew) {
    
    // Obtener el valor del tipo de servicio de forma segura
    $tipoServicio = $rsnew["tipo"] ?? '';

    // Si el tipo es esencialmente vacío, cancelar la inserción
    if (empty($tipoServicio)) {
        $this->CancelMessage = "El campo 'tipo' no puede estar vacío para generar el código de servicio.";
        return FALSE;
    }
    
    // CORRECCIÓN CRÍTICA: Sanitizar el valor antes de inyectar en la consulta SQL
    $tipoServicio_safe = adjustSql($tipoServicio); 

    // Consulta SQL para obtener el consecutivo máximo para este tipo
    $sql = "
        SELECT 
            MAX(
                CONVERT(
                    SUBSTRING_INDEX(
                        -- Inicia la subcadena después de la longitud exacta del tipo
                        SUBSTRING(Nservicio, LENGTH(tipo) + 1), 
                        '-',
                        -1
                    ),
                    UNSIGNED INTEGER
                )
            ) + 1 AS CODIGO
        FROM sco_servicio 
        WHERE tipo = '" . $tipoServicio_safe . "';
    ";
    
    $CODIGO = ExecuteScalar($sql);
    
    // Si la consulta devuelve NULL (es el primer registro de ese tipo), usa 1.
    $nuevoConsecutivo = intval($CODIGO ?? 1); 
    
    // Formatear el consecutivo a 6 dígitos con relleno de ceros y actualizar el campo
    $rsnew["Nservicio"] = $tipoServicio . str_pad($nuevoConsecutivo, 6, '0', STR_PAD_LEFT);

    $rsnew["nombre"] = strtoupper($rsnew["nombre"]);
    
    return TRUE;
}]]></CustomScript>
  <CustomScript ScriptType="Server" ScriptCodeType="Table" ScriptName="Row_Deleting" ScriptL1Key="sco_servicio"><![CDATA[// Row Deleting event
function Row_Deleting(&$rs) {
    
    // Obtener y sanitizar el ID del servicio a eliminar
    $servicioId = $rs["Nservicio"] ?? ''; 
    $servicioId_safe = adjustSql($servicioId);

    // Si el ID es inválido/vacío, prevenimos cualquier consulta.
    if (empty($servicioId_safe)) {
        $this->CancelMessage = "ID de servicio inválido o no encontrado.";
        return FALSE;
    }

    // --- 1. Verificar dependencias en sco_orden ---
    $sql_orden = "
        SELECT 
            COUNT(*) 
        FROM 
            sco_orden 
        WHERE 
            servicio = '" . $servicioId_safe . "';
    ";
    
    $cantidad_orden = ExecuteScalar($sql_orden);
    
    if (intval($cantidad_orden) > 0) {
        $this->CancelMessage = "Hay Órdenes de Servicios asociadas a este artículo (" . $cantidad_orden . "); no se puede eliminar.";
        return FALSE;
    }

    // --- 2. Verificar dependencias en sco_entrada_salida_detalle (Inventario) ---
    // Usamos el mismo valor sanitizado
    $sql_inventario = "
        SELECT 
            COUNT(*) 
        FROM 
            sco_entrada_salida_detalle 
        WHERE 
            articulo = '" . $servicioId_safe . "';
    ";
    
    $cantidad_inventario = ExecuteScalar($sql_inventario);
    
    if (intval($cantidad_inventario) > 0) {
        $this->CancelMessage = "Hay Entradas o Salidas de Inventario asociadas a este artículo (" . $cantidad_inventario . "); no se puede eliminar.";
        return FALSE;
    }
    
    // Si no hay dependencias, permitimos la eliminación
    return TRUE;
}]]></CustomScript>
  <CustomScript ScriptType="Server" ScriptCodeType="Table" ScriptName="Row_Deleting" ScriptL1Key="view_capillas"><![CDATA[// Row Deleting event
function Row_Deleting(&$rs) {
    
    // Obtener y sanitizar el ID del servicio (o capilla) a eliminar
    $servicioId = $rs["Nservicio"] ?? ''; 
    
    // CORRECCIÓN CRÍTICA: Sanitizar el valor antes de inyectarlo en el SQL
    $servicioId_safe = adjustSql($servicioId);

