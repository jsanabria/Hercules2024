    // 2. Optimización de Consultas (Notas y Adjuntos)
    // Usamos el ID de registro de forma segura
    $idRegistro = $this->Nlapidas_registro->CurrentValue;

    if ($idRegistro) {
        // Validación de Notas (Icono de archivo)
        if (isset($this->ListOptions->Items["new3"])) {
            $hasNotes = ExecuteScalar("SELECT COUNT(*) FROM sco_lapidas_notas WHERE lapida_registro = " . Conn()->quote($idRegistro));
            $this->ListOptions->Items["new3"]->Body = ($hasNotes > 0) 
                ? '<span class="fas fa-file-alt text-info" title="Tiene notas"></span>' 
                : '';
