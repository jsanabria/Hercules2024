  <CustomScript ScriptType="Server" ScriptCodeType="Table" ScriptName="Row_Inserting" ScriptL1Key="sco_servicio"><![CDATA[// Row Inserting event
function Row_Inserting($rsold, &$rsnew) {
    
    // Obtener y sanitizar el valor del tipo de servicio para la consulta SQL
    $tipoServicio = $rsnew["tipo"] ?? '';
    
    // Si el tipo es esencialmente vacío, cancelamos la inserción.
    if (empty($tipoServicio)) {
        $this->CancelMessage = "El tipo de servicio no puede estar vacío para generar el código.";
        return FALSE;
    }

    // Sanitizar el valor del tipo de servicio para la consulta SQL
    $tipoServicio_safe = adjustSql($tipoServicio); 

    // Consulta SQL: Busca el valor numérico máximo y le suma 1.
    // Esta consulta es estructuralmente correcta para el objetivo que tienes, 
    // pero muy dependiente del formato de Nservicio (Tipo + Consecutivo).
    $sql = "
        SELECT 
            MAX(
                CONVERT(
                    SUBSTRING_INDEX(
                        SUBSTRING(Nservicio, LENGTH(tipo) + 1),
                        '-',
                        -1
                    ),
                    UNSIGNED INTEGER
                )
            ) + 1 AS CODIGO
        FROM sco_servicio 
        WHERE tipo = '" . $tipoServicio_safe . "';
    ";
    
    $CODIGO = ExecuteScalar($sql);
    
    // Si la consulta devuelve NULL (es el primer registro de ese tipo), CODIGO será 1.
    $nuevoConsecutivo = intval($CODIGO ?? 1); 
    
    // Formatear el consecutivo a 6 dígitos con relleno de ceros y concatenar con el tipo.
    $rsnew["Nservicio"] = $tipoServicio . str_pad($nuevoConsecutivo, 6, '0', STR_PAD_LEFT);

    $rsnew["nombre"] = strtoupper($rsnew["nombre"]);
    
    return TRUE;
}]]></CustomScript>
