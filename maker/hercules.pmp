  <CustomScript ScriptType="Client" ScriptCodeType="Table" ScriptName="Startup Script" ScriptCtrlID="add" ScriptL1Key="sco_lapidas_registro"><![CDATA[// Startup Script de PHPMaker 2024
// Aplicar máscaras a teléfonos
// $("#x_telefono1, #x_telefono2").mask("(9999) 999-99-99");

// Evento Change para la Cédula del Difunto
// Evento Change para la Cédula del Difunto con Loader dinámico
$("#x_ci_difunto").on("change", function() {
    let $el = $(this);
    let valor = $el.val().trim().toUpperCase();

    // 1. Insertar el loader solo si no existe ya en el DOM
    if ($("#loader-ci").length === 0) {
        $el.after('<span id="loader-ci" class="ms-2 d-none"><i class="fas fa-spinner fa-spin text-primary"></i> Buscando...</span>');
    }
    let $loader = $("#loader-ci");

    if (valor === "") {
        $loader.addClass("d-none");
        return;
    }

    // 2. Limpieza y Validaciones de Formato previo al AJAX
    // Quitar espacios, puntos, comas y guiones para validar el contenido puramente
    let limpia = valor.replace(/[\s\.,-]/g, "");
    let primeraLetra = limpia.charAt(0);
    let prefijosValidos = ['V', 'E', 'J', 'G', 'M'];

    // Validar Prefijo
    if (!prefijosValidos.includes(primeraLetra)) {
        ew.alert("La Cédula/RIF debe comenzar con V, E, M, J o G");
        $el.val("").focus();
        return;
    }

    // Validar que el resto sean números
    let numeros = limpia.substring(1);
    if (numeros === "" || isNaN(numeros)) {
        ew.alert("Después del prefijo debe ingresar solo números");
        $el.val("").focus();
        return;
    }

    // 3. Formatear visualmente (Letra-Números) y preparar para envío
    let ciFormateada = primeraLetra + "-" + numeros;
    $el.val(ciFormateada);

    // 4. Iniciar proceso de búsqueda (AJAX)
    $loader.removeClass("d-none");
    $el.prop("readonly", true);

    $.getJSON("dashboard/buscar_parcela_ubicacion_lapida.php", { ci_difunto: ciFormateada }, function(data) {
        if (!data) return;

        // Si la cédula no existe en la base de datos
        if (data.nombre && data.nombre.includes("!!!")) {
            ew.alert(data.nombre);
            limpiarCamposUbicacion();
            return;
        }

        // Lógica de Alerta de Diseño Existente o Llenado de campos
        if (data.alerta && data.alerta !== "") {
            ew.confirm(data.alerta, function(result) {
                if (result) {
                    llenarCamposUbicacion(data);
                } else {
                    location.reload();
                }
            });
        } else {
            llenarCamposUbicacion(data);
        }
    })
    .fail(function() {
        console.error("Error al consultar la ubicación.");
        ew.alert("Hubo un error al conectar con el servidor.");
    })
    .always(function() {
        // Ocultar loader y habilitar campo siempre al finalizar
        $loader.addClass("d-none");
        $el.prop("readonly", false);
    });
});

// Función auxiliar para llenar campos
function llenarCamposUbicacion(data) {
    $("#x_nombre_difunto").val(data.nombre || "");
    $("#x_seccion").val(data.seccion || "");
    $("#x_modulo").val(data.modulo || "");
    $("#x_sub_seccion").val(data.sub_seccion || "");
    $("#x_parcela").val(data.parcela || "");
    $("#x_boveda").val(data.boveda || "");
    $("#x_contrato").val(data.contrato || "");
}

function limpiarCamposUbicacion() {
    $("#x_nombre_difunto, #x_seccion, #x_modulo, #x_sub_seccion, #x_parcela, #x_boveda, #x_contrato").val("");
}

// Evento para Maqueteo (Visualización de diseños)
$("#x_maqueteo").on("change", function() {
    myMaqueteo(this.value);
});]]></CustomScript>
